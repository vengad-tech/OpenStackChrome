{
  "manifest_version": 2,

  "name": "OpenStack Chrome",
  "description": "OpenStack extension for chrome",
  "version": "1.0",

  "permissions": [
    "<all_urls>",
    "storage",
    "notifications",
    "background"
  ],
  "browser_action": {
    "default_icon": "img/logo.png",

    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["js/jq.js", "js/stack.js","js/background.js"]
   
  }

}